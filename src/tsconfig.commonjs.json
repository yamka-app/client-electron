  "compilerOptions": {
    "outDir": "./js/commonjs",
    "allowJs": true,
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["ts/main", "ts/protocol"],
  "exclude": ["node_modules"]
}